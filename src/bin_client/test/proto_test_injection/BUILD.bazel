load("@obazl_rules_ocaml//ocaml:rules.bzl", "ocaml_module", "ocaml_ns_archive", "ocaml_signature")

ocaml_signature(
    name = "main__sig",
    src = ":main.mli",
)

ocaml_module(
    name = "main",
    sig = ":main__sig",
    struct = ":main.ml",
)

# okapi:auto
